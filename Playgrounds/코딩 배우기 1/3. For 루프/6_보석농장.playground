func doubleSwitch() {
    turnLeft()
    moveForward()
    toggleSwitch()
    moveForward()
    toggleSwitch()
    turnLeft()
    turnLeft()
    moveForward()
    moveForward()    
}

func doubleJem() {
    moveForward()
    collectGem()
    moveForward()
    collectGem()
    turnLeft()
    turnLeft()
    moveForward()
    moveForward()
    turnRight()
    moveForward()    
}

for i in 1 ... 3 {
    doubleSwitch()
    doubleJem()    
}